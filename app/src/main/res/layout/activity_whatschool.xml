<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/newmenugetstarted"
    tools:context=".WhatSchool">

    <LinearLayout
        android:id="@+id/headerView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:orientation="vertical"></LinearLayout>

    <ScrollView
        android:id="@+id/scrollablContent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/footerView"
        android:layout_below="@+id/headerView"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp">

        <LinearLayout
            android:id="@+id/content"
            android:layout_width="match_parent"

            android:layout_height="match_parent"
            android:orientation="vertical">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                app:cardCornerRadius="4dp"
                app:cardElevation="16dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/swlogo"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:layout_gravity="center"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:src="@drawable/way_southwest" />

                    <ImageView
                        android:id="@+id/swbuilding"
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:src="@drawable/swbuilding" />

                    <CheckBox
                        android:id="@+id/buildingbox1"
                        android:layout_width="152dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="75dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:text="This is my School" />
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                card_view:cardCornerRadius="4dp"
                card_view:cardElevation="16dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/westlogo"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:layout_gravity="center"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:src="@drawable/way_west" />

                    <ImageView
                        android:id="@+id/westbuilding"
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:src="@drawable/westbuilding" />

                    <CheckBox
                        android:id="@+id/buildingbox2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="75dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:text="This is my School" />

                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                card_view:cardCornerRadius="4dp"
                card_view:cardElevation="16dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/eastlogo"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:layout_gravity="center"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:src="@drawable/way_east" />

                    <ImageView
                        android:id="@+id/eastbuilding"
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:src="@drawable/eastbuilding" />

                    <CheckBox
                        android:id="@+id/buildingbox3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="75dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:text="This is my School" />

                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                card_view:cardCornerRadius="4dp"
                card_view:cardElevation="16dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/flintlogo"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:layout_gravity="center"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:src="@drawable/way_flint" />

                    <ImageView
                        android:id="@+id/flintbuilding"
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:src="@drawable/flintbuilding" />

                    <CheckBox
                        android:id="@+id/buildingbox4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="75dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:text="This is my School" />

                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                card_view:cardCornerRadius="4dp"
                card_view:cardElevation="16dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/waymilogo"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:layout_gravity="center"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:src="@drawable/way_michigan" />

                    <ImageView
                        android:id="@+id/waymibuilding"
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:src="@drawable/waymichiganbuilding" />

                    <CheckBox
                        android:id="@+id/buildingbox5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="75dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="12dp"
                        android:text="This is my School" />

                </LinearLayout>
            </android.support.v7.widget.CardView>

        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/footerView"
        android:layout_width="77dp"


        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="18dp"
        android:layout_marginEnd="17dp"
        android:orientation="vertical">

        <android.support.v7.widget.CardView
            android:id="@+id/cardView"
            android:layout_width="77dp"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="17dp"
            android:layout_marginEnd="18dp"
            app:cardCornerRadius="4dp"
            app:cardElevation="24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/card5">

            <Button
                android:id="@+id/button30"
                android:layout_width="77dp"
                android:layout_height="50dp"
                android:background="@color/cardview_light_background"
                android:onClick="tonextprogram"
                android:text="Next"
                android:visibility="visible"
                tools:layout_editor_absoluteX="256dp"
                tools:layout_editor_absoluteY="503dp" />

            <Button
                android:id="@+id/button31"
                android:layout_width="77dp"
                android:layout_height="50dp"
                android:background="@color/cardview_light_background"
                android:onClick="tonextsw"
                android:text="Next"
                android:visibility="gone"
                tools:layout_editor_absoluteX="256dp"
                tools:layout_editor_absoluteY="503dp" />

            <Button
                android:id="@+id/button32"
                android:layout_width="77dp"
                android:layout_height="50dp"
                android:background="@color/cardview_light_background"
                android:onClick="tonextwest"
                android:text="Next"
                android:visibility="gone"
                tools:layout_editor_absoluteX="256dp"
                tools:layout_editor_absoluteY="503dp" />

            <Button
                android:id="@+id/button33"
                android:layout_width="77dp"
                android:layout_height="50dp"
                android:background="@color/cardview_light_background"
                android:onClick="tonexteast"
                android:text="Next"
                android:visibility="gone"
                tools:layout_editor_absoluteX="256dp"
                tools:layout_editor_absoluteY="503dp" />

            <Button
                android:id="@+id/button34"
                android:layout_width="77dp"
                android:layout_height="50dp"
                android:background="@color/cardview_light_background"
                android:onClick="tonextflint"
                android:text="Next"
                android:visibility="gone"
                tools:layout_editor_absoluteX="256dp"
                tools:layout_editor_absoluteY="503dp" />

            <Button
                android:id="@+id/button35"
                android:layout_width="77dp"
                android:layout_height="50dp"
                android:background="@color/cardview_light_background"
                android:onClick="tonextwaymi"
                android:text="Next"
                android:visibility="gone"
                tools:layout_editor_absoluteX="256dp"
                tools:layout_editor_absoluteY="503dp" />

        </android.support.v7.widget.CardView>

    </LinearLayout>

</RelativeLayout>